
<table>
    <tr>
    <th>ID</th>
    <th>Name </th>
    <th>UnitPrice</th>
    <th>UnitInStock </th>
    <th>CategoryID</th>
    </tr>

    @foreach (var item in @Model)
    {
        <tr>
            <td> @item.Id </td>
            <td> @item.Name </td>
            <td> @item.UnitPrice </td>
            <td> @item.UnitInStock </td>
            <td> @item.CategoryID</td>
        </tr>
    }
</table>